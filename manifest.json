{
  "name": "YouTube Subscriptions",
  "version": "0.7",
  "manifest_version": 2,
  "description": "Application shows newest movies from authors you're subscribed to on Youtube. Stay notified up to date!",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": ["js/jquery/jquery.js", "src/background.js"]
  },
  "browser_action": {
    "default_popup": "src/popup.html"
  },
  "default_locale": "en",
  "options_page": "src/options/index.html",
  "permissions": [
    "https://www.youtube.com/*",
    "https://accounts.google.com/ServiceLogin?service=youtube*",
    "notifications"
  ]
}
